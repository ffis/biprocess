{
	"db": {
		"database": "",
		"username": "",
		"password": "",
		"options": {
			"dialect": "mssql",
			"host": "localhost",
			"operatorsAliases": false,
			"logging": false,
			"dialectOptions": {
				"encrypt": false,
				"options": {
					"enableArithAbort": true
				}
			}
		}
	},
    "mongodb": {
        "enabled": true,
		"url": "mongodb://user:pass@server:27017/db",
        "database": "db",
        "options": {"useNewUrlParser": true, "useUnifiedTopology": true}
    },
	"redis": {
		"host": "localhost",
		"port": 6379,
		"no_ready_check": true,
		"channels": {
			"listen": ["create", "update", "delete"]
		}
	},
	"debug": false
}
